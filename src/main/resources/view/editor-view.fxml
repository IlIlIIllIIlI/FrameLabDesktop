<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.frameLab.frameSprite.controller.EditorController"
            prefHeight="400.0" prefWidth="600.0" >
    <VBox spacing="40.0">
        <HBox>
            <Button onAction="#handleExport">Export</Button>
        </HBox>
        <ImageView fx:id="projectImage" fitHeight="600" fitWidth="600" pickOnBounds="true" preserveRatio="true">
        </ImageView>
    </VBox>
</AnchorPane>
